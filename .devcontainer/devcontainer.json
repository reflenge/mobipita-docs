  "dockerComposeFile": "docker-compose.yml",
  "service": "docs",
  "workspaceFolder": "/workspaces/mobipita-docs",
  "postCreateCommand": "corepack enable && pnpm install",
  "forwardPorts": [4321],
  "customizations": {
